{
  "name": "primes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "clear-wasm": "rm -f -r primes-rs/pkg",
    "build-wasm": "npm run clear-wasm && RUSTFLAGS='--cfg getrandom_backend=\"wasm_js\"' wasm-pack build primes-rs --scope caiocampos",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2",
    "@angular/common": "^19.2",
    "@angular/compiler": "^19.2",
    "@angular/core": "^19.2",
    "@angular/forms": "^19.2",
    "@angular/platform-browser": "^19.2",
    "@angular/platform-browser-dynamic": "^19.2",
    "@angular/router": "^19.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2",
    "@angular/cli": "^19.2",
    "@angular/compiler-cli": "^19.1",
    "@types/jasmine": "~5.1",
    "jasmine-core": "~5.6",
    "karma": "~6.4",
    "karma-chrome-launcher": "~3.2",
    "karma-coverage": "~2.2",
    "karma-jasmine": "~5.1",
    "karma-jasmine-html-reporter": "~2.1",
    "typescript": "~5.7"
  }
}
